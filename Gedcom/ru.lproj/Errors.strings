// relationships //
"Target is missing for key %@ (should be a %@)" = "Значение для ключа %@ не указано (должно быть %@)";
"Target %@ is incorrect type for key %@ (should be %@)" = "Значение %@ имеет недопустимый тип %@ (должен быть %@)";

/****** Sanity checking errors *******/

"%@ usually occurs before %@" = "%@ обычно встречается до %@";
