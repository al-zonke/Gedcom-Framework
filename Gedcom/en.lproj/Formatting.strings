/* GCAge period strings */
"%d year" = "%d year";
"%d years" = "%d years";
"%d month" = "%d month";
"%d months" = "%d months";
"%d day" = "%d day";
"%d days" = "%d days";

/* GCAge keywords */
"CHILD" = "Child";
"STILLBORN" = "Stillborn";
"INFANT" = "Infant";

/* GCDate keywords */
"FROM %@ TO %@" = "%$1@ … %$2@";
"FROM %@" = "%@ …";
"TO %@" = "… %@";

"BET %@ AND %@" = "%$1@ – %$2@";
"BEF %@" = "< %@";
"AFT %@" = "> %@";

"ABT %@" = "~ %@";
"EST %@" = "~ %@"; //TODO
"CAL %@" = "= %@";

"INT %@ %@" = "\"%$1@\" %$2@";

/* GCGender keywords */
"Male" = "Male";
"Female" = "Female";
"Unknown" = "Unknown";

/* GCBool keywords */
"Yes" = "Yes";
"Undecided" = "Undecided";

