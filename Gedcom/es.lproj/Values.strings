/* GCAge period strings */
"%d year" = "%d año";
"%d years" = "%d años";
"%d month" = "%d mes";
"%d months" = "%d meses";
"%d day" = "%d día";
"%d days" = "%d días";

/* GCAge keywords */
"CHILD" = "niño";
"STILLBORN" = "nacido muerto";
"INFANT" = "bebé";

/* GCDate keywords */
"FROM %@ TO %@" = "de %@ a %@";
"FROM %@" = "de %@";
"TO %@" = "%@ a";

"BET %@ AND %@" = "";
"BEF %@" = "antes de %@";
"AFT %@" = "después de %@";

"ABT %@" = "sobre %@";
"EST %@" = "aproximadamente en %@";
"CAL %@" = "calculado %@";

"INT %@ %@" = "\"%$1@\" %$2@";

/* GCGender keywords */
"Male" = "Hombre";
"Female" = "Mujer";
"Unknown" = "Desconocido";

/* GCBool keywords */
"Yes" = "Sí";
"Undecided" = "Indeciso";

